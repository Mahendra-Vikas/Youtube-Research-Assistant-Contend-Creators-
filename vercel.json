{
    "buildCommand": "cd frontend && npm install && npm run build",
    "outputDirectory": "frontend/.next",
    "framework": "nextjs",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/index.py"
        }
    ],
    "functions": {
        "api/index.py": {
            "runtime": "python3.9",
            "memory": 1024,
            "maxDuration": 10
        }
    }
} 