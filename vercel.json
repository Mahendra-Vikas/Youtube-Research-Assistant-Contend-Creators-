{
    "version": 2,
    "builds": [
        {
            "src": "frontend/next.config.ts",
            "use": "@vercel/next"
        },
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
} 